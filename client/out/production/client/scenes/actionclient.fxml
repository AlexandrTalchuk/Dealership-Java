<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ActionClientController">
   <children>
      <Button fx:id="add" layoutX="48.0" layoutY="106.0" mnemonicParsing="false" onAction="#addOrder" prefHeight="38.0" prefWidth="137.0" text="Сделать заказ" />
      <Button layoutX="232.0" layoutY="48.0" mnemonicParsing="false" onAction="#viewOrders" prefHeight="38.0" prefWidth="137.0" text="Посмотреть заказы" textAlignment="CENTER" wrapText="true" />
      <Label fx:id="name" layoutX="40.0" layoutY="31.0" />
      <Button layoutX="497.0" layoutY="241.0" mnemonicParsing="false" onAction="#logOut" prefHeight="29.0" prefWidth="66.0" text="Выйти" />
      <Button layoutX="48.0" layoutY="48.0" mnemonicParsing="false" onAction="#toSell" prefHeight="38.0" prefWidth="137.0" text="Продать автомобиль" />
      <Button layoutX="418.0" layoutY="48.0" mnemonicParsing="false" onAction="#queryForALot" prefHeight="38.0" prefWidth="137.0" text="Запрос на кредит" />
      <Button layoutX="232.0" layoutY="106.0" mnemonicParsing="false" onAction="#feedback" prefHeight="38.0" prefWidth="137.0" text="Обратная связь" />
   </children>
</Pane>
