      <Button layoutX="63.0" layoutY="202.0" mnemonicParsing="false" onAction="#Sellcars" prefHeight="59.0" prefWidth="122.0" text="Принять автомобиль" textAlignment="CENTER" wrapText="true" />
      <Button layoutX="223.0" layoutY="202.0" mnemonicParsing="false" onAction="#ApproveALoan" prefHeight="59.0" prefWidth="122.0" text="Запросы на кредит" textAlignment="CENTER" />
    </children>
</Pane>
